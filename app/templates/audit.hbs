<h1>Audit Logs</h1>

{{#if this.model.length}}
  <table class="audit-log-table">
    <thead>
      <tr>
        <th>Event Type</th>
        <th>Entity</th>
        <th>Description</th>
        <th>Timestamp</th>
      </tr>
    </thead>
    <tbody>
      {{#each this.model as |auditLog|}}
        <tr>
          <td>{{auditLog.eventType}}</td>
          <td>{{auditLog.entity}}</td>
          <td>{{auditLog.description}}</td>
          <td>{{auditLog.timestamp}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <p>No audit logs available.</p>
{{/if}}
